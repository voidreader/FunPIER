<?xml version="1.0" encoding="utf-8" ?>
<!-- 電車７人がけ上級 -->
<kyscript>
  
  <frame count ="0"/>
  <useAndShow id ="omae0" res ="densya/densya7OmaeNomal" pos ="-70,-10,0"/>
  <useAndShow id ="fuki" res="densya/densya7Fukidasi" pos ="105,66,1"/>
  <useAndShow id ="message" res ="densya/densya7Suwaruzo" pos ="127,71,0"/>
  <beginSound name ="jg_date" loop="false"/>


  <frame count ="90"/>
  <unuse id ="omae0"/>
  <unuse id ="fuki"/>
  <unuse id ="message"/>
  
  <beginSound name ="bgm_train" loop="true"/>
  <useAndShow id ="bg" res ="densya/densyaBG" pos ="0,0,5"/>
  <useAndShow id ="cm" res ="densya/densyaKoukoku" pos ="150,60,4"/>
  <useAndShow id ="turikawa" res="densya/densyaTurikawa" pos ="0,158,2"/>


  <useAndShow id ="hito1" res ="densya/densyaHitoSuwari" pos ="-250,-60,3"/>
  <useAndShow id ="hito2" res ="densya/densyaHitoSuwari" pos ="230,-60,3"/>
  <useAndShow id ="hito3" res ="densya/densyaHitoSuwari" pos ="70,-60,3"/>

  <useAndShow id ="omae" res ="densya/densyaOmaeWalkL" pos ="290,-70,0"/>
  <beginMove id ="omae" to ="180,-70" duration="30"/>

  <frame count ="120"/>

  <beginDrag id ="omae" rewind ="true" range="-220, -70, 200, -70"/>
  <beginAnim id ="omae" anim="0"/>

  <beginIf id ="goal1" target ="object" trigger="up" name="omae" crange=" -170,-60,80,120" goto ="1000"/>
  <beginIf id ="goal2" target ="object" trigger="up" name="omae" crange=" -90,-60,80,120" goto ="2000"/>
  <beginIf id ="goal3" target ="object" trigger="up" name="omae" crange=" -10,-60,80,120" goto ="3000"/>
  <beginIf id ="goal4" target ="object" trigger="up" name="omae" crange=" 150,-60,60,120" goto ="4000"/>
  <beginIf id ="goal5" target ="object" trigger="up" name="omae" crange=" -240,-60,60,120" goto ="5000"/>

  <beginTimer id ="timer0" duration="600"/>
  <beginIf id="timeout0" target="animend" name ="timer0" goto="6000" priority="1"/>
  <wait />


  <!-- 最左 -->
  <frame count ="1000"/>
  <endDrag id ="omae"/>
  <endIf id ="all"/>
  <unuse id ="timeout0"/>
  <beginMove id ="omae" to ="-170,-70" duration="30"/>
  <frame count ="1030"/>
  <unuse id ="omae"/>
  <useAndShow id ="omae2" res ="densya/densyaOmaeSuwari" pos ="-170,-60,3"/>
  <beginSound name ="se_sit"/>
  <frame count ="1120"/>
  <halt result="0"/>
  <!-- 左の真ん中 -->
  <frame count ="2000"/>
  <endDrag id ="omae"/>
  <endIf id ="all"/>
  <unuse id ="timeout0"/>
  <beginMove id ="omae" to ="-90,-70" duration="30"/>
  <frame count ="2030"/>
  <unuse id ="omae"/>
  <useAndShow id ="omae2" res ="densya/densyaOmaeSuwari" pos ="-90,-60,3"/>
  <beginSound name ="se_sit"/>
  <frame count ="2120"/>
  <halt result="1"/>

  <!-- 左の右 -->
  <frame count ="3000"/>
  <endDrag id ="omae"/>
  <endIf id ="all"/>
  <unuse id ="timeout0"/>
  <beginMove id ="omae" to ="-10,-70" duration="30"/>
  <frame count ="3030"/>
  <unuse id ="omae"/>
  <useAndShow id ="omae2" res ="densya/densyaOmaeSuwari" pos ="-10,-60,3"/>
  <beginSound name ="se_sit"/>
  <frame count ="3120"/>
  <halt result="0"/>

  <!-- 右の間(読まない) -->
  <frame count ="4000"/>
  <endDrag id ="omae"/>
  <endIf id ="all"/>
  <unuse id ="timeout0"/>
  <beginMove id ="omae" to ="150,-70" duration="30"/>
  <frame count ="4030"/>
  <unuse id ="omae"/>
  <useAndShow id ="omae2" res ="densya/densyaOmaeSuwari" pos ="150,-60,3"/>
  <beginSound name ="se_sit"/>
  <frame count ="4120"/>
  <halt result="2"/>

  <!-- 隣の車両へ -->
  <frame count ="5000"/>
  <endDrag id ="omae"/>
  <endIf id ="all"/>
  <unuse id ="timeout0"/>
  <beginMove id ="omae" to ="-300,-70" duration="30"/>
  <frame count ="5120"/>
  <var name="secret" op="set" value="1"/>
  <halt result="1"/>


  <!-- 時間切れ -->
  <frame count ="6000"/>
  <endDrag id ="omae"/>
  <endIf id ="all"/>
  <unuse id ="timeout0"/>
  <beginMove id ="omae" to ="-300,-70" duration="120"/>
  <frame count ="6120"/>
  <halt result="0"/>


  
</kyscript>