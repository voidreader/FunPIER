<!-- 衝撃告白 -->
<kyscript>

  <!-- イントロ -->
  <frame count ="0"/>
  <useAndShow id ="ako" res="syougeki/scoopAko" pos="80,0,2"/>
  <useAndShow id ="telop" res="syougeki/scoopTelop" pos="-84,208,0"/>
  <useAndShow id ="mosaic" res="syougeki/scoopMosaic" pos="140,120,1"/>
  <beginSound name ="bgm_night" volume="0.5" loop="true"/>
  <frame count ="60"/>
  <beginDrag id="mosaic" range="-200,-130,200,200"/>
  <beginIf id="cond1" target="object" name="mosaic" crange="30,70,40,20" goto="1000"/>
  <beginIf id="cond2" target="object" name="mosaic" crange="130,-50,30,30" goto="2000"/>
 
  <!-- 時間切れ -->
  <frame count ="660"/>
  <halt result ="0"/>
  
  <!-- モザイク掛け成功 -->
  <frame count ="1000"/>
  <endIf/>
  <endDrag id="mosaic"/>
  <endSound fade="0"/>
  <beginSound name ="se_bikkuri"/>
  <frame count ="1120"/>
  <halt result ="1"/>

  <!-- マイクにモザイク -->
  <frame count ="2000"/>
  <endIf/>
  <endDrag id="mosaic"/>
  <endSound fade="0"/>
  <beginSound name ="se_bikkuri"/>
  <frame count ="2120"/>
  <var name="secret" op="set" value="1"/>
  <halt result ="0"/>
  
</kyscript>